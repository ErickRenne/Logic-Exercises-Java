
public class MassaCorpórea {
	private int idade;
	private Double altura;
	private Double peso;
	private boolean genero;
	private Double resultado;
	
	public MassaCorpórea(Double peso, Double altura) {
		this.peso = peso;
		this.altura = altura;
	}
	
	
	public Double IMC() {
		resultado = (altura*2)/peso;
		return resultado;
	}

	@Override
	public String toString() {
		return "MassaCorpórea [resultado=" + resultado + "]";
	}
	
}
