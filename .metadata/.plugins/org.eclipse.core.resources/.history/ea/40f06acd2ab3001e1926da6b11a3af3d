
public class MassaCorpórea {
	private int idade;
	private int altura;
	private float peso;
	private boolean genero;
	private int resultado;
	
	public MassaCorpórea(int peso, int altura) {
		this.peso = peso;
		this.altura = altura;
	}
	
	
	public int IMC() {
		resultado = (altura*2)/peso;
		return resultado;
	}

	@Override
	public String toString() {
		return "MassaCorpórea [resultado=" + resultado + "]";
	}
	
}
